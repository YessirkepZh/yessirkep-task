{
  "name": "nuxt_seed",
  "version": "1.0.0",
  "description": "seed for new projects based on nuxtJs",
  "author": "unknown",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
