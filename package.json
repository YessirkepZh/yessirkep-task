    "axios": "^0.21.1",
    "centrifuge": "^2.7.1",
    "core-js": "^3.8.3",
    "nuxt": "^2.14.12",
    "typescript": "^4.1.5",
    "vue-pdf": "^4.3.0",
    "vuetify": "^2.4.4"
